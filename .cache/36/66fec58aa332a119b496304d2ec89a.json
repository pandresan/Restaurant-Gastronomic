-width: 500px;\n  }\n.logo1 {\nwidth: 100%;\nmax-width: 360px;\n}\n  .nav-principal {\n    margin: 0; }\n    .nav-principal ul {\n      display: flex;\n      justify-content: center; }\n      .nav-principal ul a {\n        border-top: black solid 2px; }\n  \n\n.big-div-flex-footer {\n  display: flex;\n  justify-content: space-evenly;\n  flex-direction: column; }\n  @media (min-width: 600px) {\n    .big-div-flex-footer {\n      flex-direction: row; } }\n\n/*usar mixin aquí*/\n.small-div-flex-footer {\n  display: flex;\n  justify-content: space-evenly;\n  flex-direction: column; }\n  @media (min-width: 1200px) {\n    .small-div-flex-footer {\n      flex-direction: row; } }\n\n.footer-4-children {\n  margin-left: 3rem;\n  margin-right: 3rem; }\n\n.icons {\n  display: flex;\n  justify-content: center; }\n\n\n/*MAIN INDEX*/\nfigure img {\n  width: 100%;\n  max-width: 700px; }\n\n.nav-index {\n  display: flex;\n  flex-direction: column;\n  justify-content: center; }\n  @media (min-width: 820px) {\n    .nav-index {\n      flex-direction: row; } }\n\n@media (min-width: 820px) {\n  .nav-index img {\n    width: 275px; } }\n\n@media (min-width: 970px) {\n  .nav-index img {\n    width: 325px; } }\n\n@media (min-width: 1200px) {\n  .nav-index img {\n    width: 375px; } }\n\n.nav-index a p {\n  margin: 0 2rem 4rem; }\n\n.clip {\n  clip-path: polygon(0% 10%, 4% 10%, 4% 5%, 8% 5%, 8% 0%, 92% 0%, 92% 5%, 96% 5%, 96% 10%, 100% 10%, 100% 90%, 96% 90%, 96% 95%, 92% 95%, 92% 100%, 8% 100%, 8% 95%, 4% 95%, 4% 90%, 0% 90%);\n}\n\n/*MAIN CATEGORÍA & DETALLE & PRESENTACIÓN*/\n.menus img, .detalle img {\n  max-width: 500px; }\n\n\n\n.presentacion p {\n  text-align: justify;\n  margin-left: 10%;\n  margin-right: 10%;\n}\n\n.detalle p, .detalle li, .carta dl {\n  text-align: justify;\n  margin-left: 10%;\n  margin-right: 10%;\n}\n\n.enlaces {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-evenly;\n}\n.enlaces figure {\n  max-width: 400px;\n}\n\n.enlaces img {\n  clip-path: circle(40% at center);\n}\n\n/*TRANSICIONES*/\n.platos, #plato {\n  position:relative;\n  height:400px;\n  width:100%;\n  margin:0 auto;\n}\n\n.platos img, #plato img, #plato picture {\n  position:absolute;\n    left: 0;\n    right: 0;\n    bottom: 8%;\n  margin-left: auto;\n  margin-right: auto;\n  text-align: center;\n  transition: opacity 2.5s ease-in-out;\n}\n\n.platos img.top:hover, #plato img.top:hover, #plato picture.top:hover {\n  opacity:0;\n}\n\n/*ANIMACIONES*/\n@keyframes lima-limon {\n  0% {\n    background-color: lightgray;\n  }\n  33% {\n    background-color: blue;\n  }\n  66% {\n    background-color: yellowgreen;\n  }\n  100% {\n    background-color: lightgray;\n  }\n}\n\n.logo1 {\n  animation-duration: 5s;\n  animation-name: lima-limon;\n  animation-iteration-count: infinite;\n}\n\n@keyframes borde {\n  0% {\n    border: dotted blue 1px;\n  }\n  33% {\n    border: dashed blue 1px;\n  }\n  66% {\n    border: solid blue 1px;\n  }\n  100% {\n    border: dotted blue 1px;\n  }\n}\n\n.icons img {\n  animation-duration: 2s;\n  animation-name: borde;\n  animation-iteration-count: infinite;\n  padding: 1rem;\n  border-radius: 10%;\n\n}"},"lineCount":null}},"error":null,"hash":"a0b1de8483fec13fedf8ade646888dd1","cacheData":{"env":{}}}